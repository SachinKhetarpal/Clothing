@model List<Product>
@{
    ViewBag.Title = "ProductTable";
}


<div>
    <form method="post" action="ProductTable">
        <input name="Search" />
        <button type="button" id="searchBtn">Search</button>
        <button type="button" id="newBtn">New</button>
    </form>
</div>
<table>
    <thead>
        <tr>
            <td>Product Name</td>
            <td>Category</td>
            <td>Price</td>
            <td>Actions</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in @Model)
        {
            <tr>
                <td>@product.Name</td>
                <td>-</td>
                <td>@product.Price</td>
                <td>
                    <button>Edit</button>
                    <button>Delete</button>
                </td>
            </tr>
        }
    </tbody>
</table>
<script>
    alert("hello");
    $("#searchBtn").click(function() {
        alert("button clicked");
    });

</script>
